@{
    ViewBag.Title = "Details_ThuongHieu";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
@section header{
    <h2>Chi tiết thương hiệu</h2>
    <fieldset>
        
        <div class="display-label">Mã thương hiệu  @Model.MaTH</div>
        <div class="display-label">tên thương hiệu  @Model.TenTH</div>
    </fieldset>
    @using (Html.BeginForm())
    {
        <p>
            @Html.ActionLink("Xóa", "Delete_ThuongHieu", new { id = Model.MaTH })
            @Html.ActionLink("Sửa", "Edit_ThuongHieu", new { id = Model.MaTH })
            @Html.ActionLink("Back to list ", "ThuongHieucSP")
        </p>
    }
}
